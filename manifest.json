{
    "manifest_version": 2,
    "name": "KF Sync",
    "version": "0.1",
    "permissions": [
        "activeTab",
        "cookies",
        "declarativeContent",
        "storage",
        "https://www.keyforgegame.com/",
        "*://*.decksofkeyforge.com/"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html"
    }
}